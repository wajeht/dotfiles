{
  "clg": {
    "prefix": "clg",
    "body": [
      "console.log('$1:', $1);"
    ],
    "description": "Console log with variable name and value"
  },
  "vbase": {
    "prefix": "vbase",
    "body": [
      "<template>",
      "\t<div>$1</div>",
      "</template>",
      "",
      "<script setup lang=\"ts\">",
      "$2",
      "</script>",
      "",
      "<style scoped>",
      "$3",
      "</style>"
    ],
    "description": "Vue 3 base template with TypeScript"
  },
  "vref": {
    "prefix": "vref",
    "body": [
      "const ${1:variable} = ref<${2:type}>(${3:initialValue});"
    ],
    "description": "Vue ref with TypeScript"
  }
}
